#include<stdio.h>
#include<math.h>
int main(){
		int a=0;
		int b=0;
			printf("Giai phuong trinh bac 1:");
			printf("Moi nhap a :");
			scanf("%d",&a);
			printf("Moi nhap b:");
			scanf("%d",&b);
			if(a==0){
				if(b==0){
					printf("Phuong trinh co co so nghiem");
				}else{
					printf("Phuong trinh vo nghiem");
				}
			}else{
				printf("x=%2f",(float)-b/a);
			}
	return 0;
}
